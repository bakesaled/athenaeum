{
  "name": "athenaeum",
  "version": "0.1.7",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prestart": "yarn doc",
    "doc": "ts-node -P ./tools/tsconfig.scripts.json tools/build-all-components-html.ts && yarn prettier:doc",
    "prettier:doc": "prettier --write \"projects/sample/src/app/layout/components/**/*.html\" \"projects/sample/src/assets/**/*.json\"",
    "prebuild": "yarn doc",
    "build": "ng build",
    "build:ci": "yarn build --prod",
    "build:ci:sample": "yarn build --prod --project=sample && cp dist/sample/index.html dist/sample/404.html",
    "test": "yarn ng test",
    "test:ci": "yarn test --runInBand --ci --logHeapUsage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "standard-version"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.9",
    "@angular/cdk": "^11.0.4",
    "@angular/common": "~11.0.9",
    "@angular/compiler": "~11.0.9",
    "@angular/core": "~11.0.9",
    "@angular/forms": "~11.0.9",
    "@angular/material": "^11.0.4",
    "@angular/platform-browser": "~11.0.9",
    "@angular/platform-browser-dynamic": "~11.0.9",
    "@angular/router": "~11.0.9",
    "@datorama/akita": "^6.0.0",
    "@datorama/akita-ngdevtools": "^6.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^11.0.0",
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular/cli": "~11.0.7",
    "@angular/compiler-cli": "~11.0.9",
    "@types/fs-extra": "^9.0.6",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.1",
    "cp": "^0.2.0",
    "he": "^1.2.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^26.6.3",
    "ng-packagr": "^11.0.3",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "standard-version": "^9.1.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.0.5"
  }
}
